<%include ./partials/header%>

<div class="container">
	<h1>Login</h1>
	
		<form action="/login" method="POST">
			<div class="form-group">
				<label for="exampleInputEmail1">Username</label>
				<input class="form-control" type="username" name="username" placeholder="username" id="exampleInputEmail1">
			</div>
			<div class="form-group">
				<label for="exampleInputPassword1">Password</label>
				<input class="form-control" type="password" name="password" placeholder="password" id="exampleInputPassword1">
			</div>
			<div class="form-check">
				<input type="submit" value="login" class="form-check-input btn btn-primary">
			</div>
		</form>
	
</div>


